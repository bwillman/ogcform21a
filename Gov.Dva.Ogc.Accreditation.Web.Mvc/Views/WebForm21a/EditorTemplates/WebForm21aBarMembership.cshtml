@model Gov.Dva.Ogc.Data.Accreditation.Web.Model.WebForm21aBarMembership

@{
    Layout = null;
}

                @{ 
                    String controlName = Html.NameFor(model => model.Jurisdiction).ToString();
                    Int32 brackPos = controlName.IndexOf("[");
                    Int32 jCount = Convert.ToInt32(controlName.Substring(brackPos + 1, 1)) + 1;
                    }
            <tr>
                <td colspan="3" style="padding: 0px 0px 0px 10px">
                    <div>Jurisdiction #@jCount.ToString()</div>
                </td>
            </tr>
	  		<tr>
	  			<td>
                @Html.TextBoxFor(model => model.Jurisdiction, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.Jurisdiction)
	  			</td>
	  			<td>
                @Html.TextBoxFor(model => model.AdmissionDate, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.AdmissionDate)

	  			</td>
	  			<td>
                @Html.TextBoxFor(model => model.MembershipNo, new { @class = "form-control" })
                @Html.ValidationMessageFor(model => model.MembershipNo)

	  			</td>
	  		</tr>
   
    
    
    
